<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Amount_From_1000_To_5000</fullName>
    <active>true</active>
    <description>Amount load can be from 1000 to 5000</description>
    <errorConditionFormula>Amount__c &lt; 1000 || Amount__c &gt; 5000</errorConditionFormula>
    <errorMessage>Amount must be between 1000 and 5000</errorMessage>
</ValidationRule>
