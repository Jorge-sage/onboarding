<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Months_From_6_To_12</fullName>
    <active>true</active>
    <description>Months must be between 6 and 12</description>
    <errorConditionFormula>Months__c &lt; 6 || Months__c &gt; 12</errorConditionFormula>
    <errorMessage>Number of months to return the loan are between 6 and 12</errorMessage>
</ValidationRule>
